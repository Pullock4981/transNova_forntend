{
  "name": "sdg-8-backend",
  "version": "1.0.0",
  "description": "Backend for youth-oriented job discovery and skill-development platform aligned with SDG 8",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed:jobs": "node src/seed/jobs.js",
    "seed:resources": "node src/seed/resources.js",
    "seed:all": "node src/seed/jobs.js && node src/seed/resources.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose"
  },
  "keywords": [
    "express",
    "mongodb",
    "jwt",
    "sdg-8",
    "jobs",
    "skills"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  }
